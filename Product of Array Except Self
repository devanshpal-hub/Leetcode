

class Solution {
public:
    vector<int> productExceptSelf(vector<int>& nums) {
        int n=nums.size(),t=1;
        vector<int>a(n,1);
        for (int i=0 ; i<n ; i++) {
            a[i]=t;
            t*=nums[i];
        }
        t=1;
        for (int i=n-1 ; i>=0 ; i--) {
            a[i]*=t;
            t*=nums[i];
        }
        return a;
    }
};
