

class Solution {
public:
    int majorityElement(vector<int>& nums) {
        int n=nums.size();
        int count=1,target=nums[0];
        for (int i=1 ; i<n ; i++) {
            if (nums[i]==target) {
                count++;
            }
            else {
                if (count!=0) count--;
            }
            if (count==0) {target=nums[i];count++;}
        }
        return target;
    }
};
