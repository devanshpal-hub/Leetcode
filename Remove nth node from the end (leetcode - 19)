//using iteration

ListNode* removeNthFromEnd(ListNode* head, int n) {
        int count=0;
        ListNode *temp=head;
        while(temp!=NULL){
            temp=temp->next;
            count++;
        }
        if(count==n) return head->next;
        temp=head;
        for(int i=1;i<count-n;i++)
        {
            temp=temp->next;
        }
        temp->next=temp->next->next;
        return head;
    }
