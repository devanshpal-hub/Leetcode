

class Solution {
public:
    int lengthOfLastWord(string s) {
        int count=0,n=s.size();
        for (int i=n-1 ; i>=0 ; i--) {
            if (s[i]!=' ') {
                count++;
                if (i-1>-1 && s[i-1]==' ') {
                    break;
                }
            }
            
        }
        return count;
    }
};
