

class Solution {
public:
    int lengthOfLongestSubstring(string s) {
        int maxlen=0;
        int n=s.size();
        for (int i=0 ; i<n ; i++) {
            int arr[256]={-1};
            int len=0;
            for (int j=i ; j<n ; j++) {
                if (arr[s[j]]!=-1) break;
                else {
                    arr[s[j]]=1;
                    len=j-i+1;
                    maxlen=max(maxlen,len);
                }
            }
        }
        return maxlen;
    }
};
